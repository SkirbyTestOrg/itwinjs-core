{
  "name": "@bentley/extension-client",
  "version": "2.0.0-dev.45",
  "description": "IModel.js Extension Client",
  "main": "lib/extension-client.js",
  "typings": "lib/extension-client",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/imodeljs/imodeljs/tree/master/clients/extension"
  },
  "scripts": {
    "build": "tsc 1>&2 && node ./node_modules/@bentley/build-tools/scripts/ignoreargs.js 1>&2",
    "clean": "rimraf lib .rush",
    "cover": "",
    "docs": "node ./node_modules/@bentley/build-tools/scripts/docs.js --includes=../../generated-docs/extract --json=../../generated-docs/clients/extension/file.json --tsIndexFile=./ExtensionClient.ts --onlyJson %TYPEDOC_THEME%",
    "extract-api": "node ./node_modules/@bentley/build-tools/scripts/extract-api.js --entry=ExtensionClient",
    "lint": "tslint --project . 1>&2",
    "test": "",
    "test:integration": "npm run webpackTests && npm run test:integration:chrome",
    "test:integration:chrome": "node ./node_modules/@bentley/certa/bin/certa.js -r chrome",
    "test:integration:electron": "node ./node_modules/@bentley/certa/bin/certa.js -r electron",
    "test:integration:node": "node ./node_modules/@bentley/certa/bin/certa.js -r node",
    "webpackTests": "webpack --config ./src/test/utils/webpack.config.js 1>&2"
  },
  "keywords": [
    "Bentley",
    "BIM",
    "iModel",
    "Extension"
  ],
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "dependencies": {
    "@bentley/imodeljs-clients": "2.0.0-dev.45",
    "@bentley/imodeljs-common": "2.0.0-dev.45",
    "@bentley/bentleyjs-core": "2.0.0-dev.45"
  },
  "devDependencies": {
    "@bentley/build-tools": "2.0.0-dev.45",
    "@bentley/certa": "2.0.0-dev.45",
    "@bentley/config-loader": "2.0.0-dev.45",
    "@bentley/oidc-signin-tool": "2.0.0-dev.45",
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "@types/rimraf": "^2.0.2",
    "@types/node": "10.14.1",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "nyc": "^14.0.0",
    "rimraf": "^3.0.2",
    "source-map-loader": "^0.2.3",
    "tslint": "^5.11.0",
    "typescript": "~3.7.4",
    "ts-node": "^7.0.1",
    "webpack": "^4.20.2"
  },
  "nyc": {
    "nycrc-path": "./node_modules/@bentley/build-tools/.nycrc"
  }
}
