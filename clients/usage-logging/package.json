{
  "name": "@bentley/usage-logging-client",
  "version": "2.0.0-dev.72",
  "description": "iModel.js Usage Logging Client",
  "main": "lib/usage-logging-client.js",
  "typings": "lib/usage-logging-client",
  "imodeljsSharedLibrary": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/imodeljs/imodeljs/tree/master/clients/usage-logging"
  },
  "scripts": {
    "build": "tsc 1>&2",
    "clean": "rimraf lib .rush",
    "extract-api": "betools extract-api --entry=usage-logging-client",
    "docs": "betools docs --includes=../../generated-docs/extract --json=../../generated-docs/clients/usage-logging-client/file.json --tsIndexFile=usage-logging-client.ts --onlyJson",
    "lint": "tslint --project . 1>&2",
    "webpackTests": "webpack --config ./src/test/utils/webpack.config.js 1>&2",
    "test": "",
    "test:integration": "npm run webpackTests && npm run test:integration:chrome",
    "test:integration:chrome": "node --max-http-header-size=16000 ./node_modules/@bentley/certa/bin/certa.js -r chrome --grep \"#integration\"",
    "test:integration:electron": "node ./node_modules/@bentley/certa/bin/certa.js -r electron --grep \"#integration\"",
    "cover": ""
  },
  "keywords": [
    "Bentley",
    "BIM",
    "iModel",
    "Usage Logging Client"
  ],
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "dependencies": {
    "@bentley/bentleyjs-core": "2.0.0-dev.72"
  },
  "peerDependencies": {
    "@bentley/itwin-client": "^2.0.0-dev.72"
  },
  "devDependencies": {
    "@bentley/build-tools": "2.0.0-dev.72",
    "@bentley/certa": "2.0.0-dev.72",
    "@bentley/config-loader": "2.0.0-dev.72",
    "@bentley/itwin-client": "2.0.0-dev.72",
    "@bentley/oidc-signin-tool": "2.0.0-dev.72",
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "@types/node": "10.14.1",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "nyc": "^14.0.0",
    "rimraf": "^3.0.2",
    "source-map-loader": "^0.2.3",
    "tslint": "^5.11.0",
    "typescript": "~3.7.4",
    "webpack": "4.42.0"
  },
  "nyc": {
    "nycrc-path": "./node_modules/@bentley/build-tools/.nycrc"
  }
}