{
  "name": "example-code-app",
  "//private": "NOTE: this is a private package that is never published",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "npm run build-code && npm run extract",
    "build-code": "tsc 1>&2",
    "clean": "rimraf lib package-deps.json ../../generated-docs",
    "copy:backend-assets": "cpx ./src/backend/assets/**/* ./lib/backend/assets",
    "copy:test-backend-assets": "cpx ./src/backend/test/assets/**/* ./lib/backend/test/assets",
    "extract": "node ../../node_modules/@bentley/bentleyjs-tools/scripts/extract.js --fileExt=ts --extractFrom=./src --recursive --out=../../generated-docs/extract",
    "docs": "",
    "pretest": "npm run copy:test-backend-assets && npm run copy:backend-assets && npm run build-code",
    "test": "node ../../node_modules/@bentley/bentleyjs-tools/scripts/test --packageRoot=\"../../\" --testDir=\"./lib/backend/test\""
  },
  "repository": {},
  "//dependencies": [
    "NOTE: example-code-app is set up like a real app. It knows what platform it runs on and what addon it needs."
  ],
  "dependencies": {
    "@bentley/bentleyjs-core": "8",
    "@bentley/geometry-core": "8",
    "@bentley/imodeljs-backend": "0",
    "@bentley/imodeljs-n_8-win32-x64": "~19.0.0",
    "@bentley/imodeljs-clients": "6",
    "@bentley/imodeljs-common": "0",
    "@bentley/imodeljs-frontend": "0",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "commander": "^2.14.1",
    "electron": "1.6.11",
    "express": "^4.16.2",
    "fs-extra": "^5.0.0",
    "fuse.js": "^3.2.0",
    "i18next": "^10.2.2",
    "i18next-browser-languagedetector": "^2.1.0",
    "i18next-xhr-backend": "^1.5.1",
    "js-base64": "^2.4.0",
    "save": "^2.3.2",
    "webpack": "^3.10.0"
  },
  "devDependencies": {
    "cpx": "^1.5.0",
    "rimraf": "^2.6.2",
    "typescript": "~2.7.2",
    "xmlhttprequest": "^1.8.0"
  }
}