{
  "name": "@bentley/geonames-extension",
  "version": "2.0.0-dev.48",
  "description": "Geolocation Extension",
  "main": "index.js",
  "scripts": {
    "build": "buildIModelJsModule",
    "clean": "rimraf lib .rush",
    "lint": "tslint -p . 1>&2",
    "test": "",
    "cover": "",
    "docs": ""
  },
  "keywords": [
    "iModel",
    "BIM",
    "extension",
    "Geolocation"
  ],
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "license": "MIT",
  "iModelJs": {
    "buildModule": {
      "type": "plugin",
      "sourceResources": [
        {
          "source": "./src/public/**/*",
          "dest": "./lib/build"
        },
        {
          "source": "./src/public/*.svg",
          "dest": "./lib/public/"
        }
      ],
      "webpack": {
        "build": "./lib/build",
        "dest": "./lib/plugin",
        "entry": "./lib/geoNames.js",
        "styleSheets": true,
        "bundleName": "geoNames"
      },
      "installTo": [
        "../../test-apps/display-test-app",
        "../../test-apps/ui-test-app"
      ]
    }
  },
  "devDependencies": {
    "@bentley/build-tools": "2.0.0-dev.48",
    "@bentley/config-loader": "2.0.0-dev.48",
    "@bentley/webpack-tools": "2.0.0-dev.48",
    "ui-test-app": "0.0.0",
    "display-test-app": "1.8.0",
    "@types/node": "10.14.1",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "null-loader": "^0.1.1",
    "rimraf": "^3.0.2",
    "tsconfig-paths": "^3.3.2",
    "tslint": "^5.11.0",
    "tslint-etc": "^1.5.2",
    "typescript": "~3.7.4",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.0",
    "svg-baker-runtime": "^1.4.2"
  },
  "dependencies": {
    "@bentley/bentleyjs-core": "2.0.0-dev.48",
    "@bentley/geometry-core": "2.0.0-dev.48",
    "@bentley/imodeljs-clients": "2.0.0-dev.48",
    "@bentley/imodeljs-common": "2.0.0-dev.48",
    "@bentley/imodeljs-i18n": "2.0.0-dev.48",
    "@bentley/imodeljs-frontend": "2.0.0-dev.48",
    "@bentley/imodeljs-quantity": "2.0.0-dev.48",
    "svg-sprite-loader": "^3.8.0"
  }
}
