{
  "name": "@bentley/projectlocation-extension",
  "version": "2.0.0-dev.30",
  "description": "Project extents and geolocation",
  "main": "index.js",
  "scripts": {
    "build": "buildIModelJsModule",
    "clean": "rimraf lib .rush",
    "lint": "tslint -p . 1>&2",
    "test": "",
    "cover": "",
    "docs": ""
  },
  "keywords": [
    "iModel",
    "BIM",
    "extension",
    "Project",
    "Extents",
    "Geolocation"
  ],
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "license": "MIT",
  "iModelJs": {
    "buildModule": {
      "type": "plugin",
      "sourceResources": [
        {
          "source": "./src/public/**/*",
          "dest": "./lib/build"
        }
      ],
      "webpack": {
        "build": "./lib/build",
        "dest": "./lib/plugin",
        "entry": "./lib/ProjectLocation.js",
        "bundleName": "projectLocation"
      },
      "pseudoLocalize": {
        "source": "./lib/build/locales/en",
        "dest": "./lib/build/locales/en-PSEUDO"
      },
      "installTo": [
        "../../test-apps/ui-test-app",
        "../../test-apps/display-test-app"
      ]
    }
  },
  "devDependencies": {
    "@bentley/certa": "2.0.0-dev.30",
    "@bentley/build-tools": "2.0.0-dev.30",
    "@bentley/webpack-tools": "2.0.0-dev.30",
    "@bentley/config-loader": "2.0.0-dev.30",
    "ui-test-app": "0.0.0",
    "display-test-app": "1.8.0",
    "@types/node": "10.14.1",
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "rimraf": "^2.6.2",
    "tsconfig-paths": "^3.3.2",
    "tslint": "^5.11.0",
    "tslint-etc": "^1.5.2",
    "typescript": "~3.7.4",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.0",
    "source-map-loader": "^0.2.3"
  },
  "dependencies": {
    "@bentley/bentleyjs-core": "2.0.0-dev.30",
    "@bentley/geometry-core": "2.0.0-dev.30",
    "@bentley/imodeljs-clients": "2.0.0-dev.30",
    "@bentley/imodeljs-common": "2.0.0-dev.30",
    "@bentley/imodeljs-i18n": "2.0.0-dev.30",
    "@bentley/imodeljs-frontend": "2.0.0-dev.30",
    "@bentley/ui-abstract": "2.0.0-dev.30"
  }
}
