{
  "name": "core-full-stack-tests",
  "//private": "NOTE: this is a private package that is never published",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "tsc 1>&2 && npm run webpack:frontend",
    "clean": "rimraf lib .rush coverage",
    "cover": "npm run test:chrome -- --cover && npm run test:electron -- --cover",
    "docs": "",
    "lint": "tslint --project . 1>&2",
    "webpack:frontend": "webpack --config webpack.config.js 1>&2",
    "test": "npm run test:chrome && npm run test:electron",
    "test:chrome": "node --max-http-header-size=16000 ./node_modules/@bentley/certa/bin/certa.js -r chrome --grep \"#integration\" --invert",
    "test:electron": "node ./node_modules/@bentley/certa/bin/certa.js -r electron --grep \"#integration\" --invert",
    "test:integration": "npm run test:integration:chrome && npm run test:integration:electron",
    "test:integration:chrome": "node --max-http-header-size=16000 ./node_modules/@bentley/certa/bin/certa.js -r chrome --grep \"#integration\"",
    "test:integration:electron": "node ./node_modules/@bentley/certa/bin/certa.js -r electron --grep \"#integration\""
  },
  "repository": {},
  "dependencies": {
    "@bentley/bentleyjs-core": "2.0.0-dev.56",
    "@bentley/express-server": "2.0.0-dev.56",
    "@bentley/geometry-core": "2.0.0-dev.56",
    "@bentley/imodeljs-backend": "2.0.0-dev.56",
    "@bentley/imodeljs-clients": "2.0.0-dev.56",
    "@bentley/imodeljs-clients-backend": "2.0.0-dev.56",
    "@bentley/imodeljs-common": "2.0.0-dev.56",
    "@bentley/imodeljs-i18n": "2.0.0-dev.56",
    "@bentley/imodeljs-frontend": "2.0.0-dev.56",
    "@bentley/imodeljs-markup": "2.0.0-dev.56",
    "chai": "^4.1.2",
    "fs-extra": "^8.1.0",
    "electron": "^8.0.0"
  },
  "devDependencies": {
    "@bentley/certa": "2.0.0-dev.56",
    "@bentley/config-loader": "2.0.0-dev.56",
    "@bentley/build-tools": "2.0.0-dev.56",
    "@bentley/oidc-signin-tool": "2.0.0-dev.56",
    "@types/chai": "^4.1.4",
    "@types/fs-extra": "^4.0.7",
    "@types/mocha": "^5.2.5",
    "@types/node": "10.14.1",
    "chai": "^4.1.2",
    "cpx": "^1.5.0",
    "glob": "^7.1.2",
    "istanbul-instrumenter-loader": "^3.0.1",
    "null-loader": "^0.1.1",
    "rimraf": "^3.0.2",
    "source-map-loader": "^0.2.3",
    "tslint": "^5.11.0",
    "tslint-etc": "^1.5.2",
    "typescript": "~3.7.4",
    "nyc": "^14.0.0",
    "webpack": "4.41.5"
  }
}
