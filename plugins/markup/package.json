    "@bentley/imodeljs-frontend": "0.189.0-dev.31",
    "svg.js": "~2.7.1"
  },
  "//peerDependencies": [
    "NOTE: peerDependencies are a standard way for npm to perform a module compatibility check"
  ],
  "peerDependencies": {
    "@bentley/bentleyjs-core": "0.189.0-dev.31"
  },
