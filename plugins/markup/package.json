  "description": "iModel.js plugin for markup",
  "license": "MIT",
  "main": "lib/markup.js",
  "scripts": {
    "build": "node ./node_modules/@bentley/webpack-tools/bin/buildIModelJsModule.js",
    "clean": "rimraf lib package-deps.json",
    "docs": "",
    "lint": "tslint --project . 1>&2",
    "test": "",
    "cover": ""
