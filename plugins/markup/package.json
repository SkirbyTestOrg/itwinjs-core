  "iModelJs": {
    "buildModule": {
      "type": "plugin",
      "webpack": {
        "dest": "./lib/webresources",
        "entry": "./lib/Markup.js",
        "bundleName": "MarkupPlugin"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/imodeljs/imodeljs"
  },
  "keywords": [
    "Bentley",
    "BIM",
    "iModel"
  ],
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
