    "@bentley/bentleyjs-core": "0.189.0-dev.31",
    "@bentley/build-tools": "0.189.0-dev.31",
    "@bentley/webpack-tools": "0.189.0-dev.31",
    "@types/node": "10.12.18",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "rimraf": "^2.6.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "~3.2.2",
