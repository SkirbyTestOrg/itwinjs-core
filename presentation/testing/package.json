    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "typemoq": "^2.1.0",
    "typescript": "~3.1.0"
  },
  "nyc": {
    "extends": "./node_modules/@bentley/build-tools/.nycrc",
    "check-coverage": true,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "lines": 100
