    "@bentley/imodeljs-native-platform-node": "~18.0.0",
    "electron-devtools-installer": "^2.2.4"
  },
  "devDependencies": {
    "@bentley/bentleyjs-tools": "^3.4.1",
    "@types/lodash": "4.14.106",
    "rimraf": "^2.6.2",
    "typescript": "~2.7.2",
    "electron": "1.6.11",
