  "devDependencies": {
    "@bentley/bentleyjs-core": "^7.3.6",
    "@bentley/bentleyjs-tools": "^2.2.0",    
    "@bentley/geometry-core": "^6.0.10",
    "@bentley/imodeljs-backend": "~0.70.1",
    "@bentley/imodeljs-clients": "^4.2.1",
    "@bentley/imodeljs-common": "~0.69.0",
    "@bentley/imodeljs-native-platform-api": "^12.0.0",
    "@types/chai": "^4.0.10",
    "@types/chai-spies": "0.0.0",
