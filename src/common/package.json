{
  "name": "@bentley/ecpresentation-common",
  "version": "0.0.5",
  "description": "Common pieces for ecpresentation packages.",
  "keywords": [
    "Bentley",
    "EC",
    "Presentation",
    "iModelJS"
  ],
  "license": "UNLICENSED",
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "main": "lib/index.js",
  "typings": "lib/index",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib package-deps.json",
    "cover": "nyc node ../../scripts/run-tests.js --coverage",
    "lint": "tslint -p ./tsconfig.json",
    "prepublishOnly": "npm run build",
    "test": "node ../../scripts/run-tests.js",
    "test+cover": "nyc node ../../scripts/run-tests.js --coverage --report",
    "validatePackageJson": "validjson ./package.json",
    "watch": "npm run test -- --watch"
  },
  "//peerDependencies": [
    "No need to repeat all of the dependencies are already expressed by frontend and backend."
  ],
  "devDependencies": {
    "@bentley/bentleyjs-core": "^7",
    "@bentley/bentleyjs-tools": "^2.2.0",
    "@bentley/imodeljs-common": "~0.77.1",
    "@types/chai": "^4.0.10",
    "@types/chai-jest-snapshot": "^1.3.0",
    "@types/chai-spies": "0.0.0",
    "@types/faker": "^4.1.0",
    "@types/mocha": "^2.2.44",
    "@types/source-map-support": "^0.4.0",
    "chai": "^4.1.2",
    "chai-jest-snapshot": "^1.3.0",
    "chai-spies": "1.0.0",
    "chokidar": "^2.0.0",
    "faker": "^4.1.0",
    "nyc": "^11.6.0",
    "rimraf": "^2.6.2",
    "source-map-support": "^0.5.0",
    "tslint": "^5.8.0",
    "ts-node": "^3.3.0",
    "tsconfig-paths": "^2.7.2",
    "typemoq": "^2.1.0",
    "typescript": "^2.6.2",
    "valid-json-cli": "^1.3.0"
  },
  "nyc": {
    "nycrc-path": "../../.nycrc",
    "temp-directory": "../../out/reports/coverage/common/.nyc_output",
    "report-dir": "../../out/reports/coverage/common"
  }
