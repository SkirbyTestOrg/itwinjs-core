{
  "name": "@bentley/ecpresentation-common",
  "version": "0.0.4",
  "description": "Common pieces for ecpresentation packages.",
  "keywords": [
    "Bentley",
    "EC",
    "Presentation",
    "iModelJS"
  ],
  "license": "UNLICENSED",
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "main": "lib/index.js",
  "typings": "lib/index",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib package-deps.json",
    "cover": "nyc npm run test:cover",
    "lint": "tslint -p ./tsconfig.json",
    "prepublishOnly": "npm run build",
    "require": "node ../../scripts/require-lib-modules.js --base ./",
    "test": "mocha --opts ../../mocha.opts **/*.test.ts",
    "test:cover": "npm run require && mocha --recursive **/*.test.ts"
  },
  "//peerDependencies": [
    "No need to repeat all of the dependencies are already expressed by frontend and backend."
  ],
  "devDependencies": {
    "@bentley/bentleyjs-core": "^7",
    "@bentley/bentleyjs-tools": "^2.2.0",
    "@bentley/imodeljs-common": "~0.76.1",
    "@types/chai": "^4.0.10",
    "@types/chai-spies": "0.0.0",
    "@types/faker": "^4.1.0",
    "@types/mocha": "^2.2.44",
    "chai": "^4.1.2",
    "chai-spies": "1.0.0",
    "faker": "^4.1.0",
    "nyc": "^11.6.0",
    "rimraf": "^2.6.2",
    "source-map-support": "^0.5.0",
    "tslint": "^5.8.0",
    "ts-node": "^3.3.0",
    "tsconfig-paths": "^2.7.2",
    "typemoq": "^2.1.0",
    "typescript": "^2.6.2"
  },
  "nyc": {
    "nycrc-path": "../../.nycrc",
    "temp-directory": "../../coverage/common/.nyc_output",
    "report-dir": "../../coverage/common"
  }
}