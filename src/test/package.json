{
  "name": "@bentley/ecpresentation-tests",
  "version": "0.0.0",
  "description": "ECPresentation tests",
  "license": "UNLICENSED",
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "private": true,
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "lint": "tslint -p ./tsconfig.json",
    "test": "concurrently --names \"BACKEND,FRONTEND,INTEGRATION\" \"npm run test:unit:backend\" \"npm run test:unit:frontend\" \"npm run test:integration\"",
    "test:unit": "concurrently --names \"BACKEND,FRONTEND\" \"npm run test:unit:backend\" \"npm run test:unit:frontend\"",
    "test:unit:frontend": "node node_modules/@bentley/bentleyjs-tools/scripts/test --packageRoot=\"../../\" --testDir=\"./lib/unit/frontend\"",
    "test:unit:backend": "node node_modules/@bentley/bentleyjs-tools/scripts/test --packageRoot=\"../../\" --testDir=\"./lib/unit/backend\"",
    "test:integration": "node node_modules/@bentley/bentleyjs-tools/scripts/test --packageRoot=\"../../\" --testDir=\"./lib/integration\""
  },
  "dependencies": {},
  "devDependencies": {
    "@bentley/bentleyjs-core": "^5.2.0",
    "@bentley/bentleyjs-tools": "^2.2.0",
    "@bentley/ecpresentation-backend": "^0.0.26",
    "@bentley/ecpresentation-frontend": "^0.0.15",
    "@bentley/imodeljs-backend": "^0.44.3",
    "@bentley/imodeljs-e_1_6_11-win32-x64": "^6.4.0",
    "@bentley/imodeljs-electronaddon": "^6.4.0",
    "@bentley/imodeljs-frontend": "^0.44.2",
    "@bentley/imodeljs-n_8_9-win32-x64": "^6.4.0",
    "@bentley/imodeljs-nodeaddon": "^6.4.0",
    "@types/chai": "^4.0.10",
    "@types/faker": "^4.1.0",
    "@types/mocha": "^2.2.44",
    "faker": "^4.1.0",
    "chai": "^4.1.2",
    "concurrently": "^3.5.0",
    "mocha": "^4.0.1",
    "nyc": "^11.4.1",
    "rimraf": "^2.6.2",
    "tsconfig-paths": "^2.6.0",
    "tslint": "^5.8.0",
    "typemoq": "^2.1.0"
  }
}