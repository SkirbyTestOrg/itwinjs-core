{
  "name": "presentation-test-app",
  "description": "A test app to demonstrate library capabilities",
  "license": "UNLICENSED",
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "version": "0.0.0",
  "engines": {
    "node": ">=10.15.0 <13.0"
  },
  "private": true,
  "scripts": {
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "npm run copy:assets && tsc -p tsconfig.backend.json",
    "build:frontend": "cross-env GENERATE_SOURCEMAP=false react-scripts build --unsafe-fast-sass",
    "copy:assets": "cpx \"./assets/**/*\" \"./lib/assets\"",
    "clean": "rimraf lib .rush",
    "docs": "npm run extract",
    "extract": "node ./node_modules/@bentley/build-tools/scripts/extract.js --fileExt=ts,tsx --extractFrom=./src --recursive --out=../../generated-docs/extract",
    "lint": "tslint -p ./tsconfig.json 1>&2",
    "electron": "run-p start:webserver start:electron",
    "start:electron": "electron ./lib/backend/main.js",
    "start:webserver": "cross-env BROWSER=none react-scripts start --unsafe-fast-sass",
    "start:backend": "node --inspect --max-http-header-size=16000 lib/backend/main.js",
    "start:servers": "run-p start:webserver start:backend",
    "test": "",
    "cover": ""
  },
  "dependencies": {
    "@bentley/bentleyjs-core": "2.0.0-dev.54",
    "@bentley/geometry-core": "2.0.0-dev.54",
    "@bentley/electron-manager": "2.0.0-dev.54",
    "@bentley/express-server": "2.0.0-dev.54",
    "@bentley/icons-generic-webfont": "^1.0.0",
    "@bentley/imodeljs-clients": "2.0.0-dev.54",
    "@bentley/imodeljs-common": "2.0.0-dev.54",
    "@bentley/imodeljs-backend": "2.0.0-dev.54",
    "@bentley/imodeljs-frontend": "2.0.0-dev.54",
    "@bentley/imodeljs-i18n": "2.0.0-dev.54",
    "@bentley/imodeljs-quantity": "2.0.0-dev.54",
    "@bentley/presentation-common": "2.0.0-dev.54",
    "@bentley/presentation-backend": "2.0.0-dev.54",
    "@bentley/presentation-frontend": "2.0.0-dev.54",
    "@bentley/presentation-components": "2.0.0-dev.54",
    "@bentley/ui-core": "2.0.0-dev.54",
    "@bentley/ui-components": "2.0.0-dev.54",
    "@bentley/ui-abstract": "2.0.0-dev.54",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "semver": "^5.5.0"
  },
  "devDependencies": {
    "@bentley/config-loader": "2.0.0-dev.54",
    "@bentley/build-tools": "2.0.0-dev.54",
    "@bentley/imodeljs-webserver": "2.0.0-dev.54",
    "@bentley/react-scripts": "3.4.1-dev.12",
    "@types/bunyan": "^1.8.4",
    "@types/react": "^16.8.0",
    "@types/react-dom": "^16.8.0",
    "autoprefixer": "^8.6.5",
    "cpx": "^1.5.0",
    "cross-env": "^5.1.4",
    "electron": "^6.1.5",
    "electron-chromedriver": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-flexbugs-fixes": "^3.3.1",
    "postcss-loader": "^2.1.6",
    "rimraf": "^3.0.2",
    "tslint": "^5.11.0",
    "tslint-etc": "^1.5.2",
    "typescript": "~3.7.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "electron 6.0.0",
      "last 4 chrome version",
      "last 4 firefox version",
      "last 4 safari version",
      "last 4 ios version",
      "last 4 ChromeAndroid version",
      "last 4 edge version",
      "not dead",
      "not <0.2%"
    ]
  }
}