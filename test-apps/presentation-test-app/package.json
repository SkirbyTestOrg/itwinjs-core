{
  "name": "presentation-test-app",
  "description": "A test app to demonstrate library capabilities",
  "license": "UNLICENSED",
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "version": "0.0.0",
  "engines": {
    "node": ">=10.16.0 <13.0"
  },
  "private": true,
  "scripts": {
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "npm run copy:assets && tsc -p tsconfig.backend.json",
    "build:frontend": "cross-env GENERATE_SOURCEMAP=false USE_FAST_SASS=true react-scripts build",
    "copy:assets": "cpx \"./assets/**/*\" ./lib/assets",
    "clean": "rimraf build lib .rush/temp/package-deps*.json",
    "docs": "npm run extract",
    "extract": "betools extract --fileExt=ts,tsx --extractFrom=./src --recursive --out=../../generated-docs/extract",
    "lint": "tslint -p ./tsconfig.json 1>&2",
    "electron": "run-p start:webserver start:electron",
    "start:electron": "electron ./lib/backend/main.js",
    "start:webserver": "cross-env BROWSER=none USE_FAST_SASS=true react-scripts start",
    "start:backend": "node --inspect --max-http-header-size=16000 lib/backend/main.js",
    "start:servers": "run-p start:webserver start:backend",
    "test": "",
    "cover": ""
  },
  "dependencies": {
    "@bentley/bentleyjs-core": "2.3.0-dev.2",
    "@bentley/geometry-core": "2.3.0-dev.2",
    "@bentley/electron-manager": "2.3.0-dev.2",
    "@bentley/express-server": "2.3.0-dev.2",
    "@bentley/icons-generic-webfont": "^1.0.12",
    "@bentley/imodeljs-common": "2.3.0-dev.2",
    "@bentley/imodeljs-backend": "2.3.0-dev.2",
    "@bentley/imodeljs-frontend": "2.3.0-dev.2",
    "@bentley/imodeljs-i18n": "2.3.0-dev.2",
    "@bentley/imodeljs-quantity": "2.3.0-dev.2",
    "@bentley/presentation-common": "2.3.0-dev.2",
    "@bentley/presentation-backend": "2.3.0-dev.2",
    "@bentley/presentation-frontend": "2.3.0-dev.2",
    "@bentley/presentation-components": "2.3.0-dev.2",
    "@bentley/ui-core": "2.3.0-dev.2",
    "@bentley/ui-components": "2.3.0-dev.2",
    "@bentley/ui-abstract": "2.3.0-dev.2",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-select": "^3.0.4",
    "semver": "^5.5.0"
  },
  "devDependencies": {
    "@bentley/config-loader": "2.3.0-dev.2",
    "@bentley/build-tools": "2.3.0-dev.2",
    "@bentley/react-scripts": "3.4.1",
    "@types/bunyan": "^1.8.4",
    "@types/react": "^16.8.0",
    "@types/react-dom": "^16.8.0",
    "@types/react-select": "^3.0.4",
    "autoprefixer": "^8.6.5",
    "cpx": "^1.5.0",
    "cross-env": "^5.1.4",
    "electron": "^8.2.1",
    "electron-chromedriver": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-flexbugs-fixes": "^3.3.1",
    "postcss-loader": "^2.1.6",
    "rimraf": "^3.0.2",
    "tslint": "^5.11.0",
    "tslint-etc": "^1.5.2",
    "typescript": "~3.7.4"
  },
  "browserslist": [
    "electron 6.0.0",
    "last 4 chrome version",
    "last 4 firefox version",
    "last 4 safari version",
    "last 4 ios version",
    "last 4 ChromeAndroid version",
    "last 4 edge version",
    "not dead",
    "not <0.2%"
  ]
}
