{
  "name": "presentation-test-app",
  "description": "A test app to demonstrate library capabilities",
  "license": "UNLICENSED",
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "version": "0.0.0",
  "engines": {
    "node": ">=10.15.0 <13.0"
  },
  "private": true,
  "scripts": {
    "build": "node ./node_modules/@bentley/webpack-tools/bin/buildIModelJsModule",
    "clean": "rimraf lib .rush",
    "docs": "npm run extract",
    "extract": "node ./node_modules/@bentley/build-tools/scripts/extract.js --fileExt=ts,tsx --extractFrom=./src --recursive --out=../../generated-docs/extract",
    "lint": "tslint -p ./tsconfig.json 1>&2",
    "electron": "electron ./lib/backend/main.js",
    "start:webserver": "node ./node_modules/@bentley/imodeljs-webserver/lib/webserver.js --port=3000 --resources=./lib/webresources/",
    "start:backend": "node --inspect --max-http-header-size=16000 lib/backend/main.js",
    "start:servers": "run-p start:webserver start:backend",
    "test": "",
    "cover": ""
  },
  "iModelJs": {
    "buildModule": {
      "type": "application",
      "sourceResources": [
        {
          "source": "./src/**/*.css",
          "dest": "./lib"
        },
        {
          "source": "./public/**/*",
          "dest": "./lib/webresources"
        }
      ],
      "webpack": {
        "dest": "./lib/webresources",
        "entry": "./lib/frontend/index.js",
        "bundleName": "main",
        "styleSheets": true,
        "htmlTemplate": "./src/frontend/index.html"
      },
      "makeConfig": {
        "dest": "./lib/webresources/config.json",
        "filter": "^imjs_"
      }
    }
  },
  "dependencies": {
    "@bentley/bentleyjs-core": "2.0.0-dev.37",
    "@bentley/geometry-core": "2.0.0-dev.37",
    "@bentley/electron-manager": "2.0.0-dev.37",
    "@bentley/express-server": "2.0.0-dev.37",
    "@bentley/icons-generic-webfont": "^1.0.0",
    "@bentley/imodeljs-clients": "2.0.0-dev.37",
    "@bentley/imodeljs-common": "2.0.0-dev.37",
    "@bentley/imodeljs-backend": "2.0.0-dev.37",
    "@bentley/imodeljs-frontend": "2.0.0-dev.37",
    "@bentley/imodeljs-i18n": "2.0.0-dev.37",
    "@bentley/imodeljs-quantity": "2.0.0-dev.37",
    "@bentley/presentation-common": "2.0.0-dev.37",
    "@bentley/presentation-backend": "2.0.0-dev.37",
    "@bentley/presentation-frontend": "2.0.0-dev.37",
    "@bentley/presentation-components": "2.0.0-dev.37",
    "@bentley/ui-core": "2.0.0-dev.37",
    "@bentley/ui-components": "2.0.0-dev.37",
    "@bentley/ui-abstract": "2.0.0-dev.37",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "semver": "^5.5.0"
  },
  "devDependencies": {
    "@bentley/config-loader": "2.0.0-dev.37",
    "@bentley/build-tools": "2.0.0-dev.37",
    "@bentley/imodeljs-webserver": "2.0.0-dev.37",
    "@bentley/webpack-tools": "2.0.0-dev.37",
    "@types/bunyan": "^1.8.4",
    "@types/react": "^16.8.0",
    "@types/react-dom": "^16.8.0",
    "autoprefixer": "^8.6.5",
    "electron": "^6.1.5",
    "electron-chromedriver": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-flexbugs-fixes": "^3.3.1",
    "postcss-loader": "^2.1.6",
    "rimraf": "^3.0.2",
    "tslint": "^5.11.0",
    "tslint-etc": "^1.5.2",
    "typescript": "~3.7.4"
  },
  "proxy": "http://localhost:5000"
}
