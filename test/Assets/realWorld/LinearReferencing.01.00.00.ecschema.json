         "strength" : "embedding",
         "strengthDirection" : "forward",
         "target" : {
            "constraintClasses" : [ "LinearReferencing.LinearlyReferencedFromToLocation" ],
            "multiplicity" : "(0..*)",
            "polymorphic" : false,
            "roleLabel" : "is owned by"
         }
      },
      "IReferent" : {
         "appliesTo" : "BisCore.Element",
         "description" : "Mix-in to be supported by Element-subclasses that can play the role of a Referent (known location along a Linear-Element).",
         "schemaItemType" : "Mixin"
      },
      "LENGTH" : {
         "persistenceUnit" : {
            "format" : "DefaultReal",
            "unit" : "M"
         },
         "precision" : 0.00010,
         "schemaItemType" : "KindOfQuantity"
      },
      "LinearlyReferencedAtLocation" : {
         "baseClass" : "LinearReferencing.LinearlyReferencedLocation",
         "customAttributes" : [
            {
               "className" : "BisCore.ClassHasHandler"
            }
         ],
         "description" : "Concrete multi-aspect class carrying 'at' linearly-referenced positions along a Linear-Element.",
         "modifier" : "sealed",
         "properties" : [
            {
               "name" : "AtPosition",
               "propertyType" : "StructProperty",
               "typeName" : "LinearReferencing.DistanceExpression"
            },
            {
               "direction" : "forward",
               "name" : "FromReferent",
               "propertyType" : "NavigationProperty",
               "relationshipName" : "LinearReferencing.LinearlyReferencedAtPositionRefersToReferent"
            }
         ],
         "schemaItemType" : "EntityClass"
      },
      "LinearlyReferencedAtPositionRefersToReferent" : {
         "description" : "Relationship indicating the referent used by a particular linearly-referenced position.",
