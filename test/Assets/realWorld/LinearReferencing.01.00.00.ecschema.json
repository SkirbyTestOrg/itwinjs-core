         "schemaItemType" : "RelationshipClass",
         "source" : {
            "constraintClasses" : [ "LinearReferencing.LinearlyReferencedFromToLocation" ],
            "multiplicity" : "(0..*)",
            "polymorphic" : true,
            "roleLabel" : "refers to"
         },
         "strength" : "referencing",
         "strengthDirection" : "forward",
         "target" : {
            "constraintClasses" : [ "LinearReferencing.IReferent" ],
            "multiplicity" : "(0..1)",
            "polymorphic" : true,
            "roleLabel" : "is referenced by"
         }
      },
      "LinearlyReferencedFromToLocation" : {
         "baseClass" : "LinearReferencing.LinearlyReferencedLocation",
         "customAttributes" : [
            {
               "className" : "BisCore.ClassHasHandler"
            }
         ],
         "description" : "Concrete multi-aspect class carrying 'from/to' linearly-referenced positions along a Linear-Element.",
         "modifier" : "sealed",
         "properties" : [
            {
               "name" : "FromPosition",
               "propertyType" : "StructProperty",
               "typeName" : "LinearReferencing.DistanceExpression"
            },
            {
               "direction" : "forward",
               "name" : "FromPositionFromReferent",
               "propertyType" : "NavigationProperty",
               "relationshipName" : "LinearReferencing.LinearlyReferencedFromPositionRefersToReferent"
            },
            {
               "name" : "ToPosition",
               "propertyType" : "StructProperty",
               "typeName" : "LinearReferencing.DistanceExpression"
            },
            {
               "direction" : "forward",
               "name" : "ToPositionFromReferent",
               "propertyType" : "NavigationProperty",
               "relationshipName" : "LinearReferencing.LinearlyReferencedToPositionRefersToReferent"
            }
         ],
         "schemaItemType" : "EntityClass"
      },
      "LinearlyReferencedLocation" : {
         "baseClass" : "BisCore.ElementMultiAspect",
         "description" : "Base class for multi-aspects carrying linearly-referenced locations.",
         "modifier" : "abstract",
         "schemaItemType" : "EntityClass"
      },
      "LinearlyReferencedToPositionRefersToReferent" : {
         "description" : "Relationship indicating the referent used by a particular linearly-referenced position.",
         "modifier" : "none",
         "schemaItemType" : "RelationshipClass",
         "source" : {
            "constraintClasses" : [ "LinearReferencing.LinearlyReferencedFromToLocation" ],
            "multiplicity" : "(0..*)",
            "polymorphic" : true,
            "roleLabel" : "refers to"
         },
         "strength" : "referencing",
         "strengthDirection" : "forward",
         "target" : {
            "constraintClasses" : [ "LinearReferencing.IReferent" ],
            "multiplicity" : "(0..1)",
            "polymorphic" : true,
            "roleLabel" : "is referenced by"
         }
      }
   },
   "name" : "LinearReferencing",
   "references" : [
      {
         "name" : "BisCore",
         "version" : "01.00.00"
      },
      {
         "name" : "CoreCustomAttributes",
