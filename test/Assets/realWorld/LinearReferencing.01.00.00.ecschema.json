         "schemaItemType" : "RelationshipClass",
         "source" : {
            "constraintClasses" : [ "LinearReferencing.LinearlyReferencedAtLocation" ],
            "multiplicity" : "(0..*)",
            "polymorphic" : true,
            "roleLabel" : "refers to"
