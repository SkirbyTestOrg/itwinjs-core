{
   "$schema" : "https://dev.bentley.com/json_schemas/ec/31/draft-01/ecschema",
   "alias" : "lr",
   "description" : "Base schema for Linear Referencing.",
   "items" : {
      "DistanceExpression" : {
         "description" : "Core structure carrying linearly-referenced information.",
         "properties" : [
            {
               "kindOfQuantity" : "LinearReferencing.LENGTH",
               "name" : "DistanceAlongFromStart",
               "propertyType" : "PrimitiveProperty",
               "typeName" : "double"
            },
            {
               "kindOfQuantity" : "LinearReferencing.LENGTH",
               "name" : "LateralOffsetFromILinearElement",
               "propertyType" : "PrimitiveProperty",
               "typeName" : "double"
            },
            {
               "kindOfQuantity" : "LinearReferencing.LENGTH",
               "name" : "VerticalOffsetFromILinearElement",
               "propertyType" : "PrimitiveProperty",
               "typeName" : "double"
            },
            {
               "kindOfQuantity" : "LinearReferencing.LENGTH",
               "name" : "DistanceAlongFromReferent",
               "propertyType" : "PrimitiveProperty",
               "typeName" : "double"
            }
         ],
         "schemaItemType" : "StructClass"
      },
      "ILinearElement" : {
         "appliesTo" : "BisCore.Element",
         "description" : "Mix-in to be supported by Element-subclasses meant to play the role of Linear-Elements.",
         "properties" : [
            {
               "description" : "Distance along value at the start of the Linear-Element, measured from an absolute zero.",
               "kindOfQuantity" : "LinearReferencing.LENGTH",
               "name" : "StartValue",
               "propertyType" : "PrimitiveProperty",
               "typeName" : "double"
            },
            {
               "direction" : "backward",
               "name" : "ILinearElementSource",
               "propertyType" : "NavigationProperty",
               "relationshipName" : "LinearReferencing.ILinearElementSourceProvidesILinearElements"
            }
         ],
         "schemaItemType" : "Mixin"
      },
      "ILinearElementSource" : {
         "appliesTo" : "BisCore.Element",
         "description" : "Mix-in to be supported by Element-subclasses meant to provide Linear-Elements.",
         "schemaItemType" : "Mixin"
      },
      "ILinearElementSourceProvidesILinearElements" : {
         "modifier" : "none",
         "schemaItemType" : "RelationshipClass",
         "source" : {
            "constraintClasses" : [ "LinearReferencing.ILinearElementSource" ],
            "multiplicity" : "(0..1)",
            "polymorphic" : true,
            "roleLabel" : "provides"
         },
         "strength" : "referencing",
         "strengthDirection" : "forward",
         "target" : {
            "constraintClasses" : [ "LinearReferencing.ILinearElement" ],
            "multiplicity" : "(0..*)",
            "polymorphic" : true,
            "roleLabel" : "is provided by"
         }
      },
      "ILinearlyLocated" : {
         "appliesTo" : "BisCore.Element",
         "description" : "Base mix-in for Elements to be linearly-referenced along Linear-Elements.",
         "properties" : [
            {
               "customAttributes" : [
                  {
                     "StatementTypes" : 3,
                     "className" : "BisCore.AutoHandledProperty"
                  }
               ],
               "direction" : "forward",
               "name" : "ILinearElement",
               "propertyType" : "NavigationProperty",
               "relationshipName" : "LinearReferencing.ILinearlyLocatedAlongILinearElement"
            }
         ],
         "schemaItemType" : "Mixin"
      },
      "ILinearlyLocatedAlongILinearElement" : {
         "description" : "Relationship indicating the Linear-Element along which concrete instances mixing-in ILInearlyLocated are located.",
         "modifier" : "none",
         "schemaItemType" : "RelationshipClass",
         "source" : {
            "constraintClasses" : [ "LinearReferencing.ILinearlyLocated" ],
            "multiplicity" : "(0..*)",
            "polymorphic" : true,
            "roleLabel" : "along"
