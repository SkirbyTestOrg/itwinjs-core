{
  "require": [
    "source-map-support/register",
    "ts-node/register"
  ],
  "include": [
    "src/**/*",
    "lib/**/*"
  ],
  "exclude": [
    "src/test/**/*",
    "lib/test/**/*",
    "lib/module/**/*",
    "**/*.d.ts",
    "**/*.d.tsx"
  ],
  "extension": [
    ".ts",
    ".tsx"
  ],
  "temp-directory": "./lib/test/coverage/.nyc_output",
  "report-dir": "./lib/test/coverage",
  "reporter": [
    "text-summary",
    "lcov",
    "cobertura"
  ],
  "cache": false
}